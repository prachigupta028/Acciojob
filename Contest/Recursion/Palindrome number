import java.util.*;
import java.io.*;

public class Main {
    public static void main(String args[]) {
       Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int c=0;int r=0;int s=0;
      c=n;
      while(n>0){
        r=n%10;
        s=(s*10)+r;
        n=n/10;
      }
      if(c==s){
        System.out.print("true");
      }
      else
        System.out.print("false");
      
    }
}
